#!/bin/bash
echo "Before assembling"

/usr/libexec/s2i/assemble
rc=$?


if [ $rc -eq 0 ]; then
    echo "After successful assembling"
    echo $IMPORT_URL
    printenv
    cd /opt/rh/go-toolset-7/root/usr/share/gocode/src && ls && cd github.com/stephenhillier/leapi && go test -v && cd $HOME
else
    echo "After failed assembling"
fi

exit $rc